import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner ro=new Scanner(System.in);
        String s=ro.nextLine().trim();
         String s1=ro.nextLine().trim();
        System.out.println(s+s1);
          // System.out.println(s.concat(s1));
    }
}
