import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner ro=new Scanner(System.in);
        int n=ro.nextInt();
        int sum=0;
        int num=n;
       
       
                while(n>0)
                {
                    int rem=n%10;
                    sum=sum+rem;
                    n=n/10;
                    
                }
            if(num%sum==0)
            {
                System.out.println("Harshad Number");
            }
        else
         {
            System.out.println("Not Harshad Number");
           
            }
        
           
             
        }
    }
